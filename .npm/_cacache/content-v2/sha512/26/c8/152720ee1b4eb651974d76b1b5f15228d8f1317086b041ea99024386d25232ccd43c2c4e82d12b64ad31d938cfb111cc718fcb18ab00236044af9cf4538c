      "gitHead" : "a9da57d56b13b47de89d00b8cbd616523d0ea9af",
      "_hasShrinkwrap" : false,
      "_npmOperationalInternal" : {
        "host" : "s3://npm-registry-packages",
        "tmp" : "tmp/eslint_5.0.0-rc.0_1528562125658_0.6293612969825881"
