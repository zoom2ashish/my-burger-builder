      "_id" : "jest-each@23.4.0",
      "_hasShrinkwrap" : false,
      "_npmOperationalInternal" : {
        "host" : "s3://npm-registry-packages",
        "tmp" : "tmp/jest-each_23.4.0_1531237944811_0.927036555145877"
